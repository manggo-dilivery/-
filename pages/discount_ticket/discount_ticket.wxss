/* pages/discount_ticket/discount_tickrt.wxss */
page{
  background: #f0eff4;
}
text{
  font-size:30rpx;
  color:#555;
}

.list{
  width:100%;
  height:auto;
}
.item{
  width:95%;
  height:auto;
  background: #fff;
  display: flex;
  align-items: center;
  border-top:1rpx solid #eee;
  padding:25rpx 0;
  margin:0 auto;
  margin-top:20rpx;
  position: relative;
  border-radius:10rpx;
}
.item image{
  width:150rpx;
  height:150rpx;
  margin-left:30rpx;
}
.item-ticket{
  margin-left:20rpx;
}
.item-ticket text:first-child{
  font-size: 30rpx;
  display: block;
  font-weight: bold;
  color:#666;
}
.item-ticket text:nth-child(2){
  margin-top:10rpx;
  font-size:50rpx;
  color:#ed2e26;
  font-weight:bold;
  display: inline-block;
  margin-top:40rpx;
}
.item-ticket text:nth-child(3){
  font-size:32rpx;
  color:#ed2e26;
  background: #fdf6f0;
  display: inline-block;
  margin-top:40rpx;
  margin-left:10rpx;
  padding: 2rpx 15rpx;
  border-radius:5rpx;
}

.item-click{
  background: #ea382a;
  width:200rpx;
  height:60rpx;
  border-radius:40rpx;
  text-align: center;
  position:absolute;
  top:50%;
  right:25rpx;
  margin-top:-30rpx;
}
.item-click text{
  color:#fff;
  line-height:60rpx;
  font-size: 30rpx;
}
/*åœ†  */
.wrap-top{
  width:20rpx;
  height:20rpx;
  background:#fff;
  overflow:hidden;
  position: absolute;
  top:0;
  left:460rpx;
}
.wrap-top .circle{
  width:100%;
  height:100%;
  border-radius: 50%;
  margin-top:-60%;
  background: #f0eff4;
}
.wrap-bottom{
  width:20rpx;
  height:20rpx;
  background: #fff;
  position: absolute;
  bottom:0;
  left:460rpx;
}
.wrap-bottom .circle{
  width:100%;
  height:100%;
  border-radius: 50%;
  background:#f0eff4;
  margin-top:60%;
}
.border{
  height:100%;width:0rpx;
  position: absolute;
  z-index: 5;
  left:468rpx;
  border-right:1rpx dotted #f0eff4;
}